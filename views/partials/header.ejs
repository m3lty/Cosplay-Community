<% include ../../public/js/main.ejs %>
<!DOCTYPE html>
<html>
<head>
  <title>Gonzocon</title>


  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
  <link href="/css/app.css" rel="stylesheet">
  <script type="text/javascript" src="/js/main.ejs"></script>

  </script>
</head>
<body>
  <header>
    <div class="nav navbar-default">
      <div class="navbar-brand">
        <a href="/">Gonzocon</a>
      </div>
      <div class="collapse navbar-collapse">
        <div class="nav navbar-nav navbar-right">
          <% if(!currentUser){ %>
          <li><a href="/login">Log In</a></li>
          <li><a href="/register">Sign Up</a></li>
        <% } else {  %>
          <li> <img class="img-responsive img-rounded avatar" src="<%= currentUser.avatar%>"</li>
          <li><a href="#">Welcome, <%= toCaps(currentUser.username) %></a> </li>
          <li><a href="/logout">Log out</a></li>
          <% } %>
        </div>
      </div>
    </div>
  </header>
